lt;span style="color: #0000ff;"&gt;&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;&lt;/span&gt;&lt;span style="color: #800000;"&gt;p&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&amp;lt;&lt;/span&gt;&lt;span style="color: #800000;"&gt;strong&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;IE6.0、FF1.06+：&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;/&lt;/span&gt;&lt;span style="color: #800000;"&gt;strong&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&amp;lt;&lt;/span&gt;&lt;span style="color: #800000;"&gt;br &lt;/span&gt;&lt;span style="color: #0000ff;"&gt;/&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;clientWidth = width + padding = 300+10&amp;times;2 = 320&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;&lt;/span&gt;&lt;span style="color: #800000;"&gt;br &lt;/span&gt;&lt;span style="color: #0000ff;"&gt;/&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;clientHeight = height + padding = 200+10&amp;times;2 = 220&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;&lt;/span&gt;&lt;span style="color: #800000;"&gt;br &lt;/span&gt;&lt;span style="color: #0000ff;"&gt;/&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;offsetWidth = width + padding + border = 300+10&amp;times;2+10&amp;times;2= 340&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;&lt;/span&gt;&lt;span style="color: #800000;"&gt;br &lt;/span&gt;&lt;span style="color: #0000ff;"&gt;/&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;offsetHeight = height + padding + border = 200+10&amp;times;2+10&amp;times;2 = 240&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;/&lt;/span&gt;&lt;span style="color: #800000;"&gt;p&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;/&lt;/span&gt;&lt;span style="color: #800000;"&gt;div&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;/&lt;/span&gt;&lt;span style="color: #800000;"&gt;body&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;   &lt;br /&gt;&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;lt;/&lt;/span&gt;&lt;span style="color: #800000;"&gt;html&lt;/span&gt;&lt;span style="color: #0000ff;"&gt;&amp;gt;&lt;/span&gt;&lt;span style="color: #000000;"&gt;  &lt;/span&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;img src="http://counter.cnblogs.com/blog/rss/1715267" width="1" height="1" alt=""/&gt;&lt;br/&gt;&lt;p&gt;本文链接：&lt;a href="http://www.cnblogs.com/reommmm/archive/2010/04/19/1715267.html" target="_blank"&gt;http://www.cnblogs.com/reommmm/archive/2010/04/19/1715267.html&lt;/a&gt;，转载请注明。&lt;/p&gt;</content></entry><entry><id>http://www.cnblogs.com/reommmm/archive/2010/04/19/1715166.html</id><title type="text">拼接url json - 小昊</title><summary type="text">/*************************************split url 取得每个value*****************************************/ //?pname=14 - 解脫&amp;author='aaa'&amp;copyright='bbb'&amp;info='cccc'' var arrurlvalue=""; var urlva...</summary><published>2010-04-19T02:19:00Z</published><updated>2010-04-19T02:19:00Z</updated><author><name>小昊</name><uri>http://www.cnblogs.com/reommmm/</uri></author><link rel="alternate" href="http://www.cnblogs.com/reommmm/archive/2010/04/19/1715166.html" /><link rel="alternate" type="text/html" href="http://www.cnblogs.com/reommmm/archive/2010/04/19/1715166.html" /><content type="html">&lt;p&gt;/*************************************split url 取得每个value*****************************************/&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; //?pname=14 - 解脫&amp;amp;author='aaa'&amp;amp;copyright='bbb'&amp;amp;info='cccc''&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var arrurlvalue="";&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var urlvalue;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var url = location.search;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; url = url.substring(1,url.length); //去掉?&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var arrurl = url.split('&amp;amp;');&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; for(var i=0;i&amp;lt;arrurl.length;i++){&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; arrurlvalue += arrurl[i].split('=')[1]+",";&amp;nbsp; //14 - 解脫,aaa,bbb,cccc&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; }&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; urlvalue = arrurlvalue.split(',')&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; //pname = urlvalue[0];&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; //pauthor= urlvalue[1];&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; //pcopyright= urlvalue[2];&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; //pinfo= urlvalue[3];&lt;br /&gt;/******************************************************************************/&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&lt;/p&gt;&lt;p&gt;/**********************split url 取得每个value 拼接json***********************/&lt;/p&gt;&lt;p&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var arrurlvalue="";&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var urlvalue ;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var url = location.search;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; url = url.substring(1,url.length);&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; // json //{pname:'14 - 解脫',pauthor:'aaa',pcopyright:'bbb',pinfo:'cccc'}&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; var arrurl = url.split('&amp;amp;');&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; arrurlvalue = "{";&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; for(var i=0;i&amp;lt;arrurl.length;i++){&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; if(i == arrurl.length-1)&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; arrurlvalue +=&amp;nbsp; arrurl[i].split('=')[0]&amp;nbsp; +":"+ "'"+ arrurl[i].split('=')[1] +"'";&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; else&amp;nbsp;&amp;nbsp; &lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; arrurlvalue +=&amp;nbsp; arrurl[i].split('=')[0]&amp;nbsp; +":"+ "'"+ arrurl[i].split('=')[1] +"'"+",";&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; }&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; arrurlvalue += "}";&lt;br /&gt;&amp;nbsp;//结果 arrurlvalue = "{pname:'14 - 解脫',pauthor:'aaa',pcopyright:'bbb',pinfo:'cccc',pduration:272}";&lt;br /&gt;/******************************************************************************/&lt;/p&gt;&lt;img src="http://counter.cnblogs.com/blog/rss/1715166" width="1" height="1" alt=""/&gt;&lt;br/&gt;&lt;p&gt;本文链接：&lt;a href="http://www.cnblogs.com/reommmm/archive/2010/04/19/1715166.html" target="_blank"&gt;http://www.cnblogs.com/reommmm/archive/2010/04/19/1715166.html&lt;/a&gt;，转载请注明。&lt;/p&gt;</content></entry><entry><id>http://www.cnblogs.com/reommmm/archive/2010/04/13/1711274.html</id><title type="text">网页最前显示 iframe - 小昊</title><summary type="text">层叠加的五条叠加法则&lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"&gt;&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;&lt;head&gt; &amp;...</summary><published>2010-04-13T10:02:00Z</published><updated>2010-04-13T10:02:00Z</updated><author><name>小昊</name><uri>http://www.cnblogs.com/reommmm/</uri></author><link rel="alternate" href="http://www.cnblogs.com/reommmm/archive/2010/04/13/1711274.html" /><link rel="alternate" type="text/html" href="http://www.cnblogs.com/reommmm/archive/2010/04/13/1711274.html" /><content type="html">&lt;p&gt;&lt;a href="http://www2.flash8.net/teach/7604.htm"&gt;层叠加的五条叠加法则&amp;nbsp;&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&amp;lt;!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "&lt;a href="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"&gt;http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd&lt;/a&gt;"&amp;gt;&lt;br /&gt;&amp;lt;html xmlns="&lt;a href="http://www.w3.org/1999/xhtml"&gt;http://www.w3.org/1999/xhtml&lt;/a&gt;"&amp;gt;&lt;br /&gt;&amp;lt;head&amp;gt;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;lt;title&amp;gt;Untitled Page&amp;lt;/title&amp;gt;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;lt;link rel="stylesheet" href="style.css" type="text/css" /&amp;gt;&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;lt;style&amp;gt;&amp;nbsp; &lt;br /&gt;&amp;nbsp; #div1 {&lt;br /&gt;&amp;nbsp;&amp;nbsp; position:absolute;&lt;br /&gt;&amp;nbsp;&amp;nbsp; z-index:1;&lt;br /&gt;&amp;nbsp;&amp;nbsp; background-color:#d2e8ff;&lt;br /&gt;&amp;nbsp;&amp;nbsp; visibility:hidden;&lt;br /&gt;&amp;nbsp;&amp;nbsp; &lt;br /&gt;&amp;nbsp; }&amp;nbsp;&amp;nbsp; &lt;br /&gt;&amp;nbsp;#test&lt;br /&gt;&amp;nbsp;{&lt;br /&gt;&amp;nbsp;&amp;nbsp;height:30px; line-height:30px;&lt;br /&gt;&amp;nbsp;}&lt;br /&gt;&amp;nbsp;&amp;lt;/style&amp;gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;lt;script type="text/javascript"&amp;gt;&lt;br /&gt;var nPageWidth,nPageHeight,nLeftWidth,nRightWidth;&lt;br /&gt;var nVideoCount;&lt;br /&gt;var pname,pauthor,pcopyright,pinfo;&lt;br /&gt;var p;&lt;br /&gt;var bLoop = false;&lt;br /&gt;var nMute = true;&lt;br /&gt;function $(id){&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; return document.getElementById(id);&lt;br /&gt;}&lt;br /&gt;window.onload = function(){&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; show();&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; getUrl();&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; MainPageLoad();&lt;br /&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;br /&gt;}&lt;br /&gt;//window.on